# TradeSync Functions Environment Variables
# Copy this file to .env and fill in your actual API keys

# ============================================
# GOOGLE AI (Required for ADK + GenAI)
# ============================================
# Use either API key or Vertex AI credentials.
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
GOOGLE_GENAI_USE_VERTEXAI=false
GOOGLE_CLOUD_PROJECT=your_gcp_project_id
GOOGLE_CLOUD_LOCATION=us-central1

# Model selection
MODEL_FLASH=gemini-3-flash-preview
MODEL_PRO=gemini-3-pro-preview
EMBEDDING_MODEL=gemini-embedding-001
EMBEDDING_DIMENSION=768

# Thinking configuration (Gemini 3 Pro)
# Choose one: GENAI_THINKING_LEVEL or GENAI_THINKING_BUDGET (do not set both)
GENAI_THINKING_LEVEL=HIGH
# GENAI_THINKING_BUDGET=4096
GENAI_INCLUDE_THOUGHTS=false
GENAI_FORCE_THINKING=false

# Temperature overrides (optional)
GENAI_TEMPERATURE_FLASH=0.7
GENAI_TEMPERATURE_PRO=0.4

# Safety settings (optional)
GENAI_SAFETY_DANGEROUS=BLOCK_LOW_AND_ABOVE
GENAI_SAFETY_HARASSMENT=BLOCK_MEDIUM_AND_ABOVE
GENAI_SAFETY_HATE_SPEECH=BLOCK_MEDIUM_AND_ABOVE
GENAI_SAFETY_SEXUAL=BLOCK_MEDIUM_AND_ABOVE

# ============================================
# OPTIONAL APIs
# ============================================
YOUTUBE_API_KEY=your_youtube_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
MARKETAUX_API_TOKEN=your_marketaux_token_here

# ============================================
# BINANCE TESTNET (GRATIS - Inget KYC!)
# ============================================
# 1. Ga till https://testnet.binance.vision/
# 2. Logga in med GitHub/Google (inga personuppgifter!)
# 3. Klicka "Generate HMAC_SHA256 Key"
# 4. Klistra in nycklarna har:

BINANCE_API_KEY=your_testnet_api_key
BINANCE_SECRET=your_testnet_secret_key
BINANCE_TESTNET=true

# VIKTIGT: BINANCE_TESTNET=true anvander gratis testnet
# Ändra till false ENDAST om du vill anvanda riktiga pengar (kraver KYC)

# ============================================
# TRADING SAFETY
# ============================================
LIVE_TRADING_ENABLED=false
# Ändra till true endast nar du är redo för automatisk trading

# Session history retention (ADK)
SESSION_EVENT_LIMIT=50

# ============================================
# AVANZA KEEP-ALIVE (Optional)
# ============================================
# URL to the Python function keep_avanza_alive endpoint
AVANZA_KEEP_ALIVE_URL=https://europe-west1-your-project.cloudfunctions.net/keep_avanza_alive
# Auto-build URL if not set (region + function name)
AVANZA_KEEP_ALIVE_REGION=us-central1
AVANZA_KEEP_ALIVE_FUNCTION=keep_avanza_alive
